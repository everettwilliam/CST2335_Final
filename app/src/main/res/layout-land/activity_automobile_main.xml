<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_automobile_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.eberhard.cst2335_final.AutomobileMainActivity">

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@drawable/low_beam"
        android:id="@+id/floatingActionButton2"
        android:layout_below="@+id/floatingActionButton6"
        android:layout_alignParentStart="true"
        android:layout_marginStart="63dp" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@drawable/trunk"
        android:id="@+id/floatingActionButton3"
        android:layout_centerVertical="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="39dp" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@drawable/fuel_pump"
        android:id="@+id/floatingActionButton"
        android:layout_alignTop="@+id/floatingActionButton3"
        android:layout_toStartOf="@+id/floatingActionButton3"
        android:layout_marginEnd="67dp" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@drawable/doors"
        android:id="@+id/floatingActionButton5"
        android:layout_marginStart="35dp"
        android:layout_below="@+id/floatingActionButton6"
        android:layout_toEndOf="@+id/floatingActionButton4" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@drawable/dome_light"
        android:id="@+id/floatingActionButton6"
        android:layout_above="@+id/floatingActionButton"
        android:layout_alignStart="@+id/floatingActionButton"
        android:layout_marginBottom="29dp" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@drawable/winshield_wiper"
        android:id="@+id/floatingActionButton4"
        android:layout_alignTop="@+id/floatingActionButton6"
        android:layout_alignStart="@+id/floatingActionButton2" />
</RelativeLayout>
